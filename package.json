{
    "name": "proverif-vscode",
    "displayName": "ProVerif",
    "description": "ProVerif Syntax Highlighting",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/georgio/proverif-vscode"
    },
    "bugs": {
        "url": "https://github.com/georgio/proverif-vscode/issues",
        "email": "me@georgio.xyz"
    },
    "icon": "img/icon.png",
    "license": "GPL 3.0",
    "publisher": "georgio",
    "engines": {
        "vscode": "^1.53.0"
    },
    "homepage": "https://github.com/georgio/proverif-vscode",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pv",
                "aliases": [
                    "ProVerif",
                    "pv"
                ],
                "extensions": [
                    ".pv",
                    ".pvl",
                    ".pcv"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pv",
                "scopeName": "source.proverif",
                "path": "./syntaxes/pv.tmLanguage.json"
            }
        ]
    }
}
